{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "globals": {
    "gtag": "readonly",
    "dataLayer": "readonly",
    "cloudflareAbTesting": "readonly",
    "$": "readonly",
    "jQuery": "readonly"
  }
}